  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "built",
    "esModuleInterop": true
  },
  "include": ["src/**/*.ts", "src/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}